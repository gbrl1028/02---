<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"   "http://ibatis.apache.org/dtd/sql-map-2.dtd">   

<sqlMap namespace="common">

	<sql id="pub_se">
		<isEqual property="serverType" compareValue="service">
   			IN ('1210110', '1210113', '1210114')
   		</isEqual>
   		<isEqual property="serverType" compareValue="service_en">
   			IN ('1210110', '1210114')
   		</isEqual>
   		<isEqual property="serverType" compareValue="stat">
   			IN ('1210110', '1210112', '1210113', '1210114')
   		</isEqual>
   		<isEqual property="serverType" compareValue="stat_emp">
   			IN ('1210110', '1210111', '1210112', '1210113', '1210114')
   		</isEqual>
	</sql>
	
	<sql id="pub_se_stat_emp">
   		IN ('1210110', '1210111', '1210112', '1210113', '1210114')
	</sql>
	
	<sql id="pub_se_stat">
   		IN ('1210110', '1210112', '1210113', '1210114')
	</sql>
	
	<delete id="deleteSearchCondition" parameterClass="map">
		<![CDATA[
		DELETE FROM $condTable$ WHERE SESSION_ID = #sessionId#
		]]>
	</delete>
	
</sqlMap>